                <!--<Style.Triggers>
                    <DataTrigger Value="True">
                        <Setter Property="Fill" Value="{StaticResource SignalGreen}"/>
                    </DataTrigger>
                </Style.Triggers>-->
            </Style>
        </ResourceDictionary>

    </Application.Resources>
</Application>